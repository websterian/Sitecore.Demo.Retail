<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <setting name="CommerceServer.Connect.CatalogsManagedByExternalCommerceSystem">
        <patch:attribute name="value">false</patch:attribute>
      </setting>
    </settings>
    <pipelines>
      <getContentEditorWarnings>
        <processor type="Sitecore.Commerce.Connect.DynamicsRetail.Pipelines.ContentEditorLicenseWarning, Sitecore.Commerce.Connect.DynamicsRetail"/>
      </getContentEditorWarnings>
    </pipelines>
  </sitecore>
</configuration>
